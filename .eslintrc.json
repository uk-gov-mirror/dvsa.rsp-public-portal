{
    "parser": "babel-eslint",
    "extends": ["airbnb-base"],
    "env": { "mocha": true },
    "rules": {
        "linebreak-style":"off",
        "no-unused-expressions": 0,
        "chai-friendly/no-unused-expressions": 2,
        "no-console": "warn"
    },
    "plugins": [
        "chai-friendly"
    ]
}
